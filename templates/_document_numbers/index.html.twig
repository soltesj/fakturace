{% extends 'base.html.twig' %}

{% block title %}Hello DocumentNumbersController!{% endblock %}

{% block body %}
    <h1 class="mb-5">
        <a href="{{ path('app_dash_board',{'company': company.id}) }}" class="text-decoration-none">
            <svg class="bi me-3 mb-1" width="30" height="30">
                <use href="#arrow-left-circle"/>
            </svg>
        </a>
        Format čísel dokladu
    </h1>

    {% for documentType in documentTypes %}
        {{ documentType.name }}<br>
    {% endfor %}
    {% for form in forms %}
       {{ form_start(form) }}
        {{ form_end(form) }}
    {% endfor %}
{% endblock %}
